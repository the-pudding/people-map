<!doctype html>
<html lang='en' data-version='{{timestamp}}'>

<head>



	{{> base/meta}}

	<link href='https://api.mapbox.com/mapbox-gl-js/v2.0.1/mapbox-gl.css' rel='stylesheet' />
	<link inline rel='stylesheet' href='bundle.css' />

	{{> base/font}}


	<script inline src='head.js'></script>

	{{> base/analytics }}

</head>

<body>

	{{> base/header }}

	<main id='content'>
		<figure id='map'></figure>
		<p id='info' class='is-center'><span><strong>Hover</strong> to see more details about each person.</span>
			<a target='_blank'><img inline src='external-link.svg'></a>
			<img inline src='x.svg'>
		</p>
		{{> base/footer }}
		{{> story/about }}
	</main>

	<script src='{{basepath}}assets/scripts/d3.v5.9.1.min.js'></script>
	<script src='https://api.mapbox.com/mapbox-gl-js/v2.0.1/mapbox-gl.js'></script>
	<script src='main.js?version={{timestamp}}' async></script>
</body>

</html>